.weui-label:after{content:" ";height:100%;position:absolute;left:0;bottom:0;right:0;border-right:1rpx solid #ebebeb;top:0}
.cellphone-ico{ background-image: url('http://wq.xinxao.com/addons/ss48_match/images/cellphone.png?1'); }
.dun-ico{ background-image: url('http://wq.xinxao.com/addons/ss48_match/images/dun.png?1'); }
.weui-input { height: 64rpx; min-height: 64rpx; line-height: 64rpx; }
.weui-select,.weui-input{ font-size:28rpx; color:#53586b; }
.weui-cell__ft .button{ padding: 0; margin: 0; color: #ff677d; min-height:64rpx; height:64rpx; border-radius: 6rpx; line-height:64rpx; background-color: transparent; }
.weui-cell__ft .button.disabled{ background-color: #f9f0f2; color: #999; }
.weui-cell__ft .button:after{ display: none; }
.weui-btn::after{display: none;}
